{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.vindicator": {
      "geometry": "Geometry.default",
      "part_visibility": [
        { "*": "!(v.minelp_is_pony ?? 0)" },
        { "rightArm": "(q.is_angry || q.is_celebrating) && !(v.minelp_is_pony ?? 0)" },
        { "leftArm": "(q.is_angry || q.is_celebrating) && !(v.minelp_is_pony ?? 0)" },
        { "rightItem": "q.is_angry && !(v.minelp_is_pony ?? 0)" },
        { "arms": "!(q.is_angry || q.is_celebrating) && !(v.minelp_is_pony ?? 0)" }
      ],
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.default" ]
    }
  }
}