{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.vindicator": {
      "geometry": "Geometry.default",
      "part_visibility": [
        { "*": "variable.minelp_is_pony == 0.0" },
        { "rightArm": "(query.is_angry || query.is_celebrating) && variable.minelp_is_pony == 0.0" },
        { "leftArm": "(query.is_angry || query.is_celebrating) && variable.minelp_is_pony == 0.0" },
        { "rightItem": "query.is_angry && variable.minelp_is_pony == 0.0" },
        { "arms": "!(query.is_angry || query.is_celebrating) && variable.minelp_is_pony == 0.0" }
      ],
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.default" ]
    }
  }
}