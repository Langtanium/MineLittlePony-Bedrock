      "scripts": {
        "variables": {
          // Minelp //
          "variable.minelp_is_pony": "public",
          "variable.minelp_race": "public",
          // Vanilla //
          "variable.item_use_normalized": "public",
          "variable.tp_melee_spear_use_attachable_rotation_z": "public"
        },
        "initialize": [
          // Minelp //
          "v.minelp_is_pony = 1;",
          "v.minelp_race = 1;",
          "v.minelp_horn_type = 0;",
          "v.minelp_muzzle_type = 1;",
          "v.minelp_tail_length = 4;",
          "v.minelp_size = 2;",
          "v.minelp_has_wings = 0;",
		  "v.minelp_biome = 0;",
          "v.minelp_item_x = 0;",
          "v.minelp_item_y = 0;",
          "v.minelp_item_z = 0;",
          // Vanilla //
          "v.tp_melee_spear_base_arm_rotation_x = -30.0;"
        ],
        "pre_animation": [
          // Minelp //
          "v.minelp_is_pony = q.property('minelp:is_pony');",
          "v.minelp_race = q.property('minelp:race');",
          "v.minelp_horn_type = (v.minelp_race == 3 || v.minelp_race == 4 || v.minelp_race == 5 || v.minelp_race == 7 || v.minelp_race == 10);",
          "v.minelp_magic_type = (v.minelp_race == 3 || v.minelp_race == 4 || v.minelp_race == 10);",
          "v.minelp_muzzle_type = q.property('minelp:muzzle_type');",
          "v.minelp_tail_length = q.property('minelp:tail_length');",
          "v.minelp_tail_shape = q.property('minelp:tail_shape');",
          "v.minelp_has_wings = (v.minelp_race == 2 || v.minelp_race == 4 || v.minelp_race == 5 || v.minelp_race == 7 || v.minelp_race == 8 || v.minelp_race == 9);",
          "v.minelp_biome = q.property('minelp:biome');",
          "v.is_holding_right = q.is_item_equipped('main_hand');",
          "v.is_holding_left = q.is_item_equipped('off_hand');",
          "v.movement_leg_angle = (Math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 20;",
          "v.x_bob = Math.sin(q.life_time * 76.776372) * 5.865;",
          "v.z_bob = Math.cos(q.life_time * 93.13244) * 2.865 + 2.865;",
          "v.attack = Math.sin((1.0 - (1.0 - v.attack_time) * (1.0 - v.attack_time)) * 180.0) * 57.3;",
          "v.attack2 = Math.sin(v.attack_time * 180.0) * 57.3;",
          "v.minelp_item_x = q.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bow')? -1.5 : 0;",
          "v.minelp_item_y = 0;",
          "v.minelp_item_z = 0;",
          // Vanilla //
          "v.num_professions = 15;",
          "v.profession_index = ((q.variant < v.num_professions) ? q.variant : 0);",
          // Vanilla Body //
          "v.tcos0 = (Math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 57.3;",
          // Vanilla Riding //
          "v.riding_y_offset_baby_scaling = q.is_baby ? -3.5 : 0.0;",
          "v.riding_y_offset_with_respect_to_player = 1.0;",
          "v.riding_y_offset_on_vehicle_where_other_entites_can_stand = q.is_riding_any_entity_of_type('minecraft:minecart', 'minecraft:boat', 'minecraft:chest_boat') ? -3.0 : 0.0;",
          "v.unscaled_riding_y_offset = v.riding_y_offset_with_respect_to_player + v.riding_y_offset_on_vehicle_where_other_entites_can_stand;",
          "v.riding_y_offset = v.riding_y_offset_baby_scaling * math.abs(v.unscaled_riding_y_offset) + v.unscaled_riding_y_offset + v.riding_y_offset_baby_scaling * 0.2f;",
          // Vanilla Items //
          "v.item_use_duration = q.is_using_item ? ((v.item_use_duration ?? 0.0) + q.delta_time * 20.0) : 0.0;",
          "v.item_use_normalized = v.item_use_duration / query.main_hand_item_max_duration;",
          // Vanilla Melee spear //
          "v.melee_spear_equipped = q.equipped_item_any_tag('slot.weapon.mainhand', 'minecraft:is_spear');",
          // Vanilla Melee spear use //
          "t.melee_spear_use_finish_raise_tick = v.melee_spear_equipped ? q.kinetic_weapon_delay : 0.0;",
          "t.melee_spear_use_finish_sway_tick = v.melee_spear_equipped ? (t.melee_spear_use_finish_raise_tick + q.kinetic_weapon_dismount_duration) : 0.0;",
          "t.melee_spear_use_start_sway_tick = v.melee_spear_equipped ? (t.melee_spear_use_finish_sway_tick - 20.0) : 0.0;",
          "t.melee_spear_use_finish_lower_tick = v.melee_spear_equipped ? (t.melee_spear_use_finish_raise_tick + q.kinetic_weapon_knockback_duration) : 0.0;",
          "t.melee_spear_use_start_lower_tick = v.melee_spear_equipped ? (t.melee_spear_use_finish_lower_tick - 40.0) : 0.0;",
          "t.melee_spear_use_finish_raise_back_tick = v.melee_spear_equipped ? (t.melee_spear_use_finish_raise_tick + q.kinetic_weapon_damage_duration) : 0.0;",
          "t.melee_spear_use_start_raise_back_tick = v.melee_spear_equipped ? (t.melee_spear_use_finish_raise_back_tick - 5.0) : 0.0;",
          "t.melee_spear_use_raise_progress = math.clamp(math.inverse_lerp(0.0, t.melee_spear_use_finish_raise_tick, v.item_use_duration), 0.0, 1.0);",
          "t.melee_spear_use_raise_progress_start = math.clamp(math.inverse_lerp(0.0, 0.5, t.melee_spear_use_raise_progress), 0.0, 1.0);",
          "t.melee_spear_use_raise_progress_middle = math.clamp(math.inverse_lerp(0.5, 0.8, t.melee_spear_use_raise_progress), 0.0, 1.0);",
          "t.melee_spear_use_raise_progress_end = math.clamp(math.inverse_lerp(0.8, 1.0, t.melee_spear_use_raise_progress), 0.0, 1.0);",
          "t.melee_spear_use_sway_progress = math.clamp(math.inverse_lerp(t.melee_spear_use_start_sway_tick, t.melee_spear_use_finish_sway_tick, v.item_use_duration), 0.0, 1.0);",
          "t.melee_spear_use_lower_progress = math.ease_in_out_elastic(0.0, 1.0, math.clamp(math.inverse_lerp(t.melee_spear_use_start_lower_tick, t.melee_spear_use_finish_lower_tick, v.item_use_duration - 20.0), 0.0, 1.0));",
          "t.melee_spear_use_raise_back_progress = math.clamp(math.inverse_lerp(t.melee_spear_use_start_raise_back_tick, t.melee_spear_use_finish_raise_back_tick, v.item_use_duration), 0.0, 1.0);",
          "t.melee_spear_use_sway_intensity = math.ease_out_circ(0.0, 2.0, t.melee_spear_use_sway_progress) + math.ease_in_circ(0.0, -2.0, t.melee_spear_use_raise_back_progress);",
          "t.melee_spear_use_sway_scale_slow = math.sin(v.item_use_duration * 19.0) * t.melee_spear_use_sway_intensity;",
          "t.melee_spear_use_sway_scale_fast = math.sin(v.item_use_duration * 30.0) * t.melee_spear_use_sway_intensity;",
          "v.tp_melee_spear_use_arm_rotation_x = t.melee_spear_use_raise_progress_start * -40.0 + t.melee_spear_use_raise_progress_middle * 30.0 + t.melee_spear_use_raise_progress_end * -20.0 + t.melee_spear_use_lower_progress * 20.0 + t.melee_spear_use_raise_back_progress * 10.0;",
          "v.tp_melee_spear_use_arm_rotation_x = v.tp_melee_spear_use_arm_rotation_x + t.melee_spear_use_sway_scale_slow * 0.6;",
          "v.tp_melee_spear_use_arm_rotation_y = t.melee_spear_use_sway_scale_fast * 1.0;",
          "v.tp_melee_spear_use_arm_rotation_z = t.melee_spear_use_sway_scale_slow * 0.5;",
          "v.tp_melee_spear_use_item_rotation_x = t.melee_spear_use_raise_progress * 60.0 + t.melee_spear_use_raise_back_progress * -60.0;",
          "v.tp_melee_spear_use_attachable_rotation_z = t.melee_spear_use_raise_progress * 90.0 + t.melee_spear_use_sway_progress * -90.0;"
        ],
        "animate": [
          // Minelp //
          { "default": "(v.minelp_is_pony ?? 0)" },
