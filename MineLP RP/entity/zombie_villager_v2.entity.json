          { "use_item_progress_controller": "!(v.minelp_is_pony ?? 0)" },
          { "zombie_attack_bare_hand_controller": "!(v.minelp_is_pony ?? 0) && !v.melee_spear_equipped" },
          { "swimming_controller": "!(v.minelp_is_pony ?? 0)" },
          { "melee_spear_controller": "v.melee_spear_equipped" }
        ],
