          "variable.minelp_is_pony = q.property('property:is_pony');",
          "variable.minelp_race = q.property('property:race');",
          "variable.minelp_horn_type = (v.minelp_race == 3 || v.minelp_race == 4 || v.minelp_race == 5 || v.minelp_race == 7 || v.minelp_race == 10);",
