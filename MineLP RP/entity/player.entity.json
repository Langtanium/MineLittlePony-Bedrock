        "holding_brush": "animation.humanoid.holding_brush",
        "brushing": "animation.humanoid.brushing"
      },
      "render_controllers": [
        // Vanilla //
        { "controller.render.player.first_person_spectator": "v.is_first_person && q.is_spectator" },
        { "controller.render.player.third_person_spectator": "!v.is_first_person && !v.map_face_icon && q.is_spectator && (v.minelp_is_pony ?? 0)" },
        { "controller.render.player.first_person": "v.is_first_person && !q.is_spectator" },
        { "controller.render.player.third_person": "!v.is_first_person && !v.map_face_icon && !q.is_spectator" },
        { "controller.render.player.map": "v.map_face_icon" },
        // Minelp //
        { "controller.render.player.cape": "q.has_cape" },
        { "controller.render.pony.first_person_griffin": "v.is_first_person && !q.is_spectator && v.minelp_race == 8 && (v.minelp_is_pony ?? 0)" },
        { "controller.render.pony.third_person_spectator": "!v.is_first_person && !v.map_face_icon && q.is_spectator" },
        { "controller.render.pony.third_person": "!v.is_first_person && !v.map_face_icon && !q.is_spectator && (v.minelp_is_pony ?? 0)" },
        { "controller.render.minelp.pony.player.magic": "(v.minelp_is_pony ?? 0) && (v.minelp_horn_type ?? 0) >= 1 && !v.is_first_person && !q.is_spectator" }
      ],
      "enable_attachables": true
