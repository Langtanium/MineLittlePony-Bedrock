      },
      "textures": {
        "default": "textures/entity/steve",
        "cape": "textures/entity/cape"
      },
      "geometry": {
        // Vanilla //
        "default": "geometry.humanoid.custom",
        "cape": "geometry.cape",
        // Minelp //
        "minelp_earth_pony": "geometry.minelp.pony.ep",
        "minelp_pegasus": "geometry.minelp.pony.pe",
