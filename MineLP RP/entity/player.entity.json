        "minelp_hippogriff": "geometry.minelp.pony.hg",
        "minelp_kirin": "geometry.minelp.pony.ki",
        "minelp_batpony": "geometry.minelp.pony.bp",
        "minelp_seapony": "geometry.minelp.pony.seapony.tail",
        // Minelp Accessories //
        "minelp_antlers": "geometry.antlers",
        "minelp_crown": "geometry.crown",
        "minelp_muffin": "geometry.muffin",
        "minelp_saddlebags": "geometry.saddlebags",
        "minelp_stetson": "geometry.stetson",
        "minelp_witch_hat": "geometry.witch_hat"
      },
      "scripts": {
        "variables": {
          "variable.minelp_is_pony": "public",
          "variable.minelp_race": "public"
        },
        "scale": "0.9375",
        "initialize": [
          "v.minelp_is_pony = 1;",
          "v.minelp_is_seapony = 0;",
          "v.minelp_race = 1;",
          "v.minelp_handedness = 0;",
          "v.minelp_horn_type = 0;",
          "v.minelp_muzzle_type = 1;",
          "v.minelp_tail_length = 4;",
          "v.minelp_size = 2;",
          "v.minelp_has_wings = 0;"
        ],
        "pre_animation": [
          // Minelp //
          "v.minelp_is_pony = q.property('property:is_pony');",
          "v.minelp_is_seapony = q.property('property:is_seapony');",
          "v.minelp_race = q.property('property:race');",
          "v.minelp_handedness = 0;",
          "v.minelp_horn_type = (v.minelp_race == 3 || v.minelp_race == 4 || v.minelp_race == 5 || v.minelp_race == 7 || v.minelp_race == 10);",
          "v.minelp_muzzle_type = q.property('property:muzzle_type');",
          "v.minelp_tail_length = q.property('property:tail_length');",
          "v.minelp_tail_shape = q.property('property:tail_shape');",
          "v.minelp_size = q.property('property:pony_size');",
          "v.minelp_has_wings = (v.minelp_race == 2 || v.minelp_race == 4 || v.minelp_race == 5 || v.minelp_race == 7 || v.minelp_race == 8 || v.minelp_race == 9);",
          "v.x_bob = Math.sin(q.life_time * 76.776372) * 5.865;",
          "v.z_bob = Math.cos(q.life_time * 93.13244) * 2.865 + 2.865;",
          "v.attack = Math.sin((1.0 - (1.0 - v.attack_time) * (1.0 - v.attack_time)) * 180.0) * 57.3;",
          "v.attack2 = Math.sin(v.attack_time * 180.0) * 57.3;",
          "v.movement_leg_angle = (Math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 20;",
          // Vanilla //
          "v.helmet_layer_visible = 1.0;",
          "v.leg_layer_visible = 1.0;",
          "v.boot_layer_visible = 1.0;",
          "v.chest_layer_visible = 1.0;",
          "v.attack_body_rot_y = Math.sin(360*Math.sqrt(v.attack_time)) * 5.0;",
          "v.tcos0 = (math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 57.3;",
          "v.first_person_rotation_factor = math.sin((1 - v.attack_time) * 180.0);",
          "v.hand_bob = q.life_time < 0.01 ? 0.0 : v.hand_bob + ((q.is_on_ground && q.is_alive ? math.clamp(math.sqrt(math.pow(q.position_delta(0), 2.0) + math.pow(q.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - v.hand_bob) * 0.02;",
          "v.map_angle = math.clamp(1 - v.player_x_rotation / 45.1, 0.0, 1.0);",
          "v.item_use_normalized = q.main_hand_item_use_duration / q.main_hand_item_max_duration;"
        ],
        "animate": [
          // Minelp //
          { "default": "(v.minelp_is_pony ?? 0) && !v.is_first_person" },
          { "fly_controller": "(v.minelp_is_pony ?? 0) && (v.minelp_has_wings ?? 0)" },
          { "arm_fix": "(v.minelp_is_pony ?? 0) && (v.minelp_horn_type ?? 0.0) == 0 && !v.is_first_person" },
          { "arm_fix_magic": "(v.minelp_is_pony ?? 0) && (v.minelp_horn_type ?? 0) >= 1 && !v.is_first_person" },
          { "bow_fix": "(v.minelp_is_pony ?? 0) && (v.minelp_horn_type ?? 0) == 0 && !v.is_first_person && q.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bow')" },
          { "bow_fix_magic": "(v.minelp_is_pony ?? 0) && (v.minelp_horn_type ?? 0) >= 1.0 && !v.is_first_person && q.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bow')" },
          { "long_neck": "(v.minelp_race ?? 0) == 6" },
          { "tail_bumpy": "(v.minelp_tail_shape ?? 0) == 1" },
          { "tail_swirly": "(v.minelp_tail_shape ?? 0) == 2" },
          { "tail_spiky": "(v.minelp_tail_shape ?? 0) == 3" },
          { "size_foal": "(v.minelp_is_pony ?? 0) && (v.minelp_size ?? 0) == 0" },
          // Vanilla //
          "root"
        ]
      },
      "animations": {
        // Minelp //
        "pony_cape_controller": "controller.animation.minelp.pony.cape",
        "pony_cape": "animation.player.pony.cape",
        "pony_cape_flap": "animation.player.pony.cape_flap",
