{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:skeleton",
      "min_engine_version": "1.12.0",
      "materials": {
        "default": "skeleton",
        "minelp_default": "skeleton",
        "minelp_magic": "pony_magic"
      },
      "textures": {
        "default": "textures/entity/skeleton/skeleton",
        "minelp_default": "textures/entity/skeleton/skeleton_pony"
      },
      "geometry": {
        "default": "geometry.skeleton.v1.8",
        "minelp_default": "geometry.minelp.pony.un.skeleton",
        "minelp_earth_pony": "geometry.minelp.pony.ep.skeleton",
        "minelp_pegasus": "geometry.minelp.pony.ep.skeleton",
        "minelp_unicorn": "geometry.minelp.pony.un.skeleton"
      },
      "spawn_egg": {
        "texture": "spawn_egg_skeleton"
      },
      "scripts": {
        "variables": {
          "variable.minelp_is_pony": "public",
          "variable.minelp_race": "public"
        },
        "initialize": [
          "v.minelp_is_pony = 1;",
          "v.minelp_race = 3;",
          "v.minelp_handedness = 0;",
          "v.minelp_horn_type = 1;",
          "v.minelp_muzzle_type = 1;",
          "v.minelp_tail_length = 4;",
          "v.minelp_has_wings = 0;",
          "v.minelp_item_x = 0;",
          "v.minelp_item_y = 0;",
          "v.minelp_item_z = 0;",
          "v.minelp_item_magic_x = 0;",
          "v.minelp_item_magic_y = 0;",
          "v.minelp_item_magic_z = 0;"
        ],
        "pre_animation": [
          // Minelp //
          "v.minelp_is_pony = q.property('minelp:is_pony');",
          "v.minelp_race = q.property('minelp:race');",
          "v.minelp_horn_type = v.minelp_race == 3;",
          "v.minelp_magic_type = v.minelp_race == 3;",
          "v.is_holding_right = q.is_item_equipped('main_hand');",
          "v.is_holding_left = q.is_item_equipped('off_hand');",
          "v.movement_leg_angle = (Math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 20;",
          "v.x_bob = Math.sin(q.life_time * 76.776372) * 5.865;",
          "v.z_bob = Math.cos(q.life_time * 93.13244) * 2.865 + 2.865;",
          "v.attack = Math.sin((1.0 - (1.0 - v.attack_time) * (1.0 - v.attack_time)) * 180.0) * 57.3;",
          "v.attack2 = Math.sin(v.attack_time * 180.0) * 57.3;",
          "v.minelp_item_x = q.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bow')? -1.5 : 0;",
          "v.minelp_item_y = 0;",
          "v.minelp_item_z = 0;",
          "v.minelp_item_magic_x = q.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bow')? -1 : 0;",
          "v.minelp_item_magic_y = 0;",
          "v.minelp_item_magic_z = 0;",
          // Vanilla //
          "v.tcos0 = (Math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 57.3;",
          "v.riding_y_offset_with_respect_to_player = 0.5;",
          "v.riding_y_offset_on_vehicle_where_other_entites_can_stand = q.is_riding_any_entity_of_type('minecraft:minecart', 'minecraft:boat', 'minecraft:chest_boat') ? -3.0 : 0.0;",
          "v.riding_y_offset = v.riding_y_offset_with_respect_to_player + v.riding_y_offset_on_vehicle_where_other_entites_can_stand;"
        ],
        "animate": [
          // Minelp //
          { "default": "(v.minelp_is_pony ?? 0)" },
          // Vanilla //
          { "look_at_target_controller": "!(v.minelp_is_pony ?? 0)" },
          { "move_controller": "!(v.minelp_is_pony ?? 0)" },
          { "riding_controller": "!(v.minelp_is_pony ?? 0)" },
          { "holding_controller": "!(v.minelp_is_pony ?? 0)" },
          { "brandish_spear_controller": "!(v.minelp_is_pony ?? 0)" },
          { "charging_controller": "!(v.minelp_is_pony ?? 0)" },
          { "attack_controller": "!(v.minelp_is_pony ?? 0)" },
          { "sneaking_controller": "!(v.minelp_is_pony ?? 0)" },
          { "bob_controller": "!(v.minelp_is_pony ?? 0)" },
          { "damage_nearby_mobs_controller": "!(v.minelp_is_pony ?? 0)" },
          { "bow_and_arrow_controller": "!(v.minelp_is_pony ?? 0)" },
          { "swimming_controller": "!(v.minelp_is_pony ?? 0)" },
          { "use_item_progress_controller": "!(v.minelp_is_pony ?? 0)" },
          { "skeleton_attack_controller": "!(v.minelp_is_pony ?? 0)" }
        ]
      },
      "animations": {
        // Minelp //
        "default": "controller.animation.minelp.pony.skeleton.default",
        "arm_fix": "animation.minelp.pony.correct_skeleton",
        "arm_fix_magic": "animation.minelp.pony.correct_skeleton_magic",
        "default.arms": "controller.animation.minelp.pony.default.arms",
        "show_movement.leftArm": "animation.minelp.pony.show_movement.leftArm",
        "show_movement.rightArm": "animation.minelp.pony.show_movement.rightArm",
        "show_movement.legs": "animation.minelp.pony.show_movement.legs",
        "show_movement.tail": "animation.minelp.pony.show_movement.tail",
        "show_attack.leftArm": "animation.minelp.pony.show_attack.leftArm",
        "show_attack.rightArm": "animation.minelp.pony.show_attack.rightArm",
        "bob.leftArm": "animation.minelp.pony.bob.leftArm",
        "bob.rightArm": "animation.minelp.pony.bob.rightArm",
        "bob.tail": "animation.minelp.pony.bob.tail",
        "look_at_target": "animation.minelp.pony.look_at_target",
        "big_head": "animation.minelp.pony.big_head",
        // Vanilla //
        "look_at_target_default": "animation.humanoid.look_at_target.default",
        "look_at_target_gliding": "animation.humanoid.look_at_target.gliding",
        "look_at_target_swimming": "animation.humanoid.look_at_target.swimming",
        "move": "animation.humanoid.move",
        "riding.body": "animation.humanoid.riding.body",
        "riding.arms": "animation.humanoid.riding.arms",
        "riding.legs": "animation.humanoid.riding.legs",
        "holding": "animation.humanoid.holding",
        "brandish_spear": "animation.humanoid.brandish_spear",
        "charging": "animation.humanoid.charging",
        "attack.rotations": "animation.humanoid.attack.rotations",
        "sneaking": "animation.humanoid.sneaking",
        "bob": "animation.humanoid.bob",
        "damage_nearby_mobs": "animation.humanoid.damage_nearby_mobs",
        "bow_and_arrow": "animation.humanoid.bow_and_arrow",
        "swimming": "animation.zombie.swimming",
        "use_item_progress": "animation.humanoid.use_item_progress",
        "skeleton_attack": "animation.skeleton.attack",
        "look_at_target_controller": "controller.animation.humanoid.look_at_target",
        "move_controller": "controller.animation.humanoid.move",
        "riding_controller": "controller.animation.humanoid.riding",
        "holding_controller": "controller.animation.humanoid.holding",
        "brandish_spear_controller": "controller.animation.humanoid.brandish_spear",
        "charging_controller": "controller.animation.humanoid.charging",
        "attack_controller": "controller.animation.humanoid.attack",
        "sneaking_controller": "controller.animation.humanoid.sneaking",
        "bob_controller": "controller.animation.humanoid.bob",
        "damage_nearby_mobs_controller": "controller.animation.humanoid.damage_nearby_mobs",
        "bow_and_arrow_controller": "controller.animation.humanoid.bow_and_arrow",
        "swimming_controller": "controller.animation.zombie.swimming",
        "use_item_progress_controller": "controller.animation.humanoid.use_item_progress",
        "skeleton_attack_controller": "controller.animation.skeleton.attack"
      },
      "render_controllers": [
        "controller.render.skeleton",
        { "controller.render.minelp.pony_variented": "(v.minelp_is_pony ?? 0)" },
        { "controller.render.minelp.pony.magic": "(v.minelp_is_pony ?? 0) && (v.minelp_magic_type ?? 0)" }
      ],
      "enable_attachables": true
    }
  }
}
